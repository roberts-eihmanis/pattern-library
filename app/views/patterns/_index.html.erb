<div class="tab_container">
  <h2>Jauns Raports</h2>
  <div>
    <div class="form-group row">
      <label for="example-text-input" class="col-2 col-form-label">Rindas</label>
      <div class="col-10">
        <input class="form-control row-boxes" type="text">
      </div>
    </div>
    <div class="form-group row">
      <label for="example-text-input" class="col-2 col-form-label">Kolonnas</label>
      <div class="col-10">
        <input class="form-control column-boxes" type="text">
      </div>
    </div>
    <button class="btn-primary btn" id="new-pattern">Jauns Raports</button>
  </div>
  <h2>Jauns Raports No Attēla</h2>
  <div class="panel panel-default">
    <div class="panel-body">

      <!-- Standar Form -->
      <h4>Izvēlies bildi no datora</h4>
      <form action="" method="post" enctype="multipart/form-data" id="js-upload-form">
        <div class="form-inline">
          <div class="form-group">
            <input type="file" name="files[]" id="js-upload-files">
          </div>
          <button type="submit" class="btn btn-sm btn-primary" id="js-upload-submit">Augšupielādet bildi</button>
        </div>
      </form>

      <!-- Drop Zone -->
      <h4>Vai ievelciet bildi šeit</h4>
      <div class="upload-drop-zone" id="drop-zone">
        Ievelc bildi šeit
      </div>
      
    </div>
  </div>
</div>

